<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/batteryButton4"
        android:layout_width="173dp"
        android:layout_height="78dp"
        android:layout_marginStart="12dp"
        android:layout_marginEnd="12dp"
        android:layout_marginBottom="17dp"
        android:background="@drawable/clear_button"
        app:layout_constraintBottom_toTopOf="@+id/status"
        app:layout_constraintEnd_toStartOf="@+id/batteryButton3"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/mapsButton"
        android:layout_width="87dp"
        android:layout_height="38dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="4dp"
        android:layout_marginEnd="176dp"
        android:layout_marginBottom="15dp"
        android:text="Maps"
        android:textSize="12sp"
        app:layout_constraintBottom_toTopOf="@+id/mainSpinner"
        app:layout_constraintEnd_toStartOf="@+id/signOutButton"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/signOutButton"
        android:layout_width="92dp"
        android:layout_height="38dp"
        android:layout_marginTop="4dp"
        android:layout_marginEnd="28dp"
        android:layout_marginBottom="15dp"
        android:text="Sign Out"
        android:textSize="12sp"
        app:layout_constraintBottom_toTopOf="@+id/mainSpinner"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/batteryButton"
        android:layout_width="173dp"
        android:layout_height="78dp"
        android:layout_marginStart="12dp"
        android:layout_marginEnd="12dp"
        android:background="@drawable/clear_button"
        app:layout_constraintBottom_toTopOf="@+id/batteryButton4"
        app:layout_constraintEnd_toStartOf="@+id/batteryButton2"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/batteryButton2"
        android:layout_width="173dp"
        android:layout_height="78dp"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="12dp"
        android:background="@drawable/clear_button"
        app:layout_constraintBottom_toTopOf="@+id/batteryButton3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/mapView" />

    <Button
        android:id="@+id/batteryButton3"
        android:layout_width="173dp"
        android:layout_height="78dp"
        android:layout_marginEnd="12dp"
        android:layout_marginBottom="16dp"
        android:background="@drawable/clear_button"
        app:layout_constraintBottom_toTopOf="@+id/statusTV"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/batteryButton2" />

    <TextView
        android:id="@+id/tempTV"
        android:layout_width="163dp"
        android:layout_height="26dp"
        android:layout_marginBottom="8dp"
        android:text="4"
        android:textAlignment="center"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="@+id/batteryButton3"
        app:layout_constraintEnd_toEndOf="@+id/batteryButton3"
        app:layout_constraintStart_toStartOf="@+id/batteryButton3" />

    <TextView
        android:id="@+id/compass"
        android:layout_width="157dp"
        android:layout_height="32dp"
        android:layout_marginBottom="8dp"
        android:text="Compass"
        android:textAlignment="center"
        android:textSize="26sp"
        app:layout_constraintBottom_toTopOf="@+id/compassTV"
        app:layout_constraintEnd_toEndOf="@+id/batteryButton2"
        app:layout_constraintStart_toStartOf="@+id/batteryButton2"
        app:layout_constraintTop_toTopOf="@+id/batteryButton2" />

    <TextView
        android:id="@+id/compassTV"
        android:layout_width="155dp"
        android:layout_height="23dp"
        android:layout_marginBottom="12dp"
        android:text="76"
        android:textAlignment="center"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="@+id/batteryButton2"
        app:layout_constraintEnd_toEndOf="@+id/batteryButton2"
        app:layout_constraintHorizontal_bias="0.444"
        app:layout_constraintStart_toStartOf="@+id/batteryButton2" />

    <TextView
        android:id="@+id/batteryTV"
        android:layout_width="163dp"
        android:layout_height="26dp"
        android:layout_marginBottom="11dp"
        android:text="50%"
        android:textAlignment="center"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="@+id/batteryButton"
        app:layout_constraintEnd_toEndOf="@+id/batteryButton"
        app:layout_constraintStart_toStartOf="@+id/batteryButton" />

    <TextView
        android:id="@+id/battery"
        android:layout_width="164dp"
        android:layout_height="33dp"
        android:layout_marginBottom="8dp"
        android:text="Battery"
        android:textAlignment="center"
        android:textSize="26sp"
        app:layout_constraintBottom_toTopOf="@+id/batteryTV"
        app:layout_constraintEnd_toEndOf="@+id/batteryButton"
        app:layout_constraintStart_toStartOf="@+id/batteryButton"
        app:layout_constraintTop_toTopOf="@+id/batteryButton" />

    <TextView
        android:id="@+id/temperature"
        android:layout_width="164dp"
        android:layout_height="33dp"
        android:text="Temperature"
        android:textAlignment="center"
        android:textSize="24sp"
        app:layout_constraintBottom_toTopOf="@+id/tempTV"
        app:layout_constraintEnd_toEndOf="@+id/batteryButton3"
        app:layout_constraintStart_toStartOf="@+id/batteryButton3"
        app:layout_constraintTop_toBottomOf="@+id/batteryButton2" />

    <TextView
        android:id="@+id/current"
        android:layout_width="164dp"
        android:layout_height="33dp"
        android:text="Current"
        android:textAlignment="center"
        android:textSize="26sp"
        app:layout_constraintBottom_toTopOf="@+id/currentTV"
        app:layout_constraintEnd_toEndOf="@+id/batteryButton4"
        app:layout_constraintStart_toStartOf="@+id/batteryButton4"
        app:layout_constraintTop_toTopOf="@+id/batteryButton4" />

    <TextView
        android:id="@+id/currentTV"
        android:layout_width="168dp"
        android:layout_height="29dp"
        android:text="12 A"
        android:textAlignment="center"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="@+id/batteryButton4"
        app:layout_constraintEnd_toEndOf="@+id/batteryButton4"
        app:layout_constraintStart_toStartOf="@+id/batteryButton4" />

    <Button
        android:id="@+id/powerButton"
        android:layout_width="177dp"
        android:layout_height="41dp"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="23dp"
        android:layout_marginBottom="29dp"
        android:text="TURN ON"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/chargeButton"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/chargeButton"
        android:layout_width="177dp"
        android:layout_height="41dp"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="29dp"
        android:text="CHARGE"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <com.google.android.gms.maps.MapView
        android:id="@+id/mapView"
        android:layout_width="320dp"
        android:layout_height="308dp"
        android:layout_marginStart="35dp"
        android:layout_marginEnd="35dp"
        android:layout_marginBottom="308dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.476"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/status"
        android:layout_width="76dp"
        android:layout_height="28dp"
        android:layout_marginStart="116dp"
        android:layout_marginTop="17dp"
        android:layout_marginEnd="15dp"
        android:layout_marginBottom="14dp"
        android:text="Status: "
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/powerButton"
        app:layout_constraintEnd_toStartOf="@+id/statusTV"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/currentTV" />

    <TextView
        android:id="@+id/statusTV"
        android:layout_width="136dp"
        android:layout_height="29dp"
        android:layout_marginEnd="68dp"
        android:layout_marginBottom="14dp"
        android:text="OFF"
        android:textSize="22sp"
        app:layout_constraintBottom_toTopOf="@+id/chargeButton"
        app:layout_constraintEnd_toEndOf="parent" />

    <Spinner
        android:id="@+id/mainSpinner"
        android:layout_width="198dp"
        android:layout_height="28dp"
        android:layout_marginStart="106dp"
        android:layout_marginEnd="107dp"
        android:layout_marginBottom="27dp"
        app:layout_constraintBottom_toTopOf="@+id/mapView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>